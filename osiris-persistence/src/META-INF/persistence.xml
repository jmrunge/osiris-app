    <class>ar.com.zir.osiris.api.personas.PersonaJuridica</class>
    <class>ar.com.zir.osiris.api.personas.Provincia</class>
    <class>ar.com.zir.osiris.api.personas.TelefonoPersona</class>
    <class>ar.com.zir.osiris.api.personas.WebPersona</class>
    <class>ar.com.zir.osiris.api.security.SecurityObject</class>
    <class>ar.com.zir.osiris.api.security.SystemGroup</class>
    <class>ar.com.zir.osiris.api.security.SystemGroupRoles</class>
    <class>ar.com.zir.osiris.api.security.SystemOption</class>
