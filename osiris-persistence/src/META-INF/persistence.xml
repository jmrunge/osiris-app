    <class>ar.com.zir.osiris.api.security.SystemUser</class>
    <class>ar.com.zir.osiris.api.security.SystemUserAdmin</class>
    <class>ar.com.zir.osiris.api.security.SystemUserPassword</class>
    <class>ar.com.zir.osiris.api.security.SystemUserPersonaFisica</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <!--jar-file>osiris-ejb-lib.jar</jar-file>
    <jar-file>osiris-ejb.jar</jar-file-->
    <properties>
      <property name="eclipselink.logging.logger" value="DefaultLogger"/>
      <property name="eclipselink.logging.level" value="INFO"/>
      <property name="eclipselink.target-server" value="SunAS9"/>
      <property name="eclipselink.cache.type.default" value="Weak"/>
      <property name="eclipselink.session.customizer" value="ar.com.zir.osiris.ejb.persistence.TableSequence"/>
      <property name="eclipselink.jdbc.sequence-connection-pool" value="true"/>
      <!--property name="eclipselink.cache.shared.default" value="false"/-->
    </properties>
